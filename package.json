{
  "name": "mern-auth",
  "version": "1.0.0",
  "description": "Mern Auth Example",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "engines": {
    "node": "12.16.3"
  },
  "author": "Le Liang",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.13",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "save": "^2.4.0",
    "sendemail": "^4.0.1",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  }
}
